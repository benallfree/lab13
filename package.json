{
  "name": "lab13-root",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "prettier --check .",
    "lint:fix": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 120,
    "quoteProps": "preserve"
  },
  "workspaces": [
    "cli",
    "sdk",
    "site",
    "games/*"
  ],
  "trustedDependencies": [
    "advzip-bin",
    "core-js",
    "core-js-pure",
    "cwebp-bin",
    "ect-bin",
    "esbuild",
    "gifsicle",
    "jpegtran-bin",
    "mozjpeg",
    "optipng-bin",
    "pngquant-bin",
    "sharp",
    "workerd"
  ],
  "patchedDependencies": {
    "roadroller@2.1.0": "patches/roadroller@2.1.0.patch"
  }
}
